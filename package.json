{
  "name": "aleph1-layout",
  "author": "Essoudry Avraham",
  "version": "1.0.0",
  "description": "A Responsive Layout library.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avrahamcool/aleph1-layout.git"
  },
  "keywords": [
    "sass",
    "aleph1",
    "layout",
    "framework",
    "ui",
    "css framework"
  ],
  "dependencies": {
    "node-sass": "*"
  },
  "scripts": {
    "watch": "node-sass ./src/scss/ --output ./dist/ --watch --recursive --source-map-embed --source-map-contents --output-style expanded | node-sass ./src/scss/main.scss ./dist/main.min.css --watch --output-style compressed",
    "test": "node-sass ./src/scss/main.scss ./docs/main.css --source-map-embed --source-map-contents --output-style expanded | http-server ./docs -p 1111 -a localhost -o -c-1"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avrahamcool/aleph1-layout/issues"
  },
  "homepage": "https://github.com/avrahamcool/aleph1-layout",
  "devDependencies": {}
}
